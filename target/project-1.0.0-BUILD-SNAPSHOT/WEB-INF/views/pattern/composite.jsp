        };
        Legion.prototype.getNumber = function() {
            let sum = 0;
            this.cohorts.forEach(function(cohort) {
                sum += cohort.getNumber();
            });
            return sum;
        }
        Legion.prototype.addCohort = function(cohort) {
            this.cohorts.push(cohort);
            return this;
        };
        return Legion;
    })();
