<script>
    $(document).ready(function() {

        // confirm테스트
        $("#confirm").on("click", function () {
            const msg = '저장?';
            getConfirm(msg, function(tf){
                if(tf) {
                    alert("확인");
                } else {
                    alert("취소");
                }
            });
        });

        // 싱글톤 실행 테스트
        $("#confirm1").on("click", function () {
            fnSingleTon();
        });

        // 코드 확인 버튼
        $("#codeCheck").on("click", function () {
            let tf = $("#codeBox").is(':visible');
            if(tf){
                $("#codeBox").hide();
            } else {
                $("#codeBox").show();
            }
        });



    });

    // 선언
    const SingleTon = (function() {
        let instance;

        const App = (function() {
            const App = function() {
                this.save = save;
            };

            const save = function(url, data) {
                return new Promise(function(resolve) {
                    const valid = validate(data);
                    if (!valid.validCheck) {
                        alert(valid.message);
                        return;
                    }

                    const confirmMessage = '등록하시겠습니까?';
                    getConfirm(confirmMessage, function(tf) {
                        if(tf){
                            const jsonData = {index:12};
                            const callBackFn = function (data){
                                alert("콜백함수");
                                resolve("then");
                            }

                            fnAjax(url,jsonData,callBackFn);
                        } else {
                            alert("취소");
                        }

                    });
                });
            };

            /* 유효성 검사*/
            const validate = function(data) {
                let validCheck = false;
                let message = '';

                if(data.valid){
                    validCheck = true;
                } else {
                    message += getMessage(data.state) + '<br>';
                }

                return {
                    validCheck: validCheck,
                    message: message
                };

            };

            /*유효성 검사시 에러코드 반환*/
            const getMessage = function(key) {
                const message = {
                    first: 'first error situation',
                    second: 'second error situation',
                };
                return message[key];
            }

            return App;
        })();


        return {
            getInstance: function() {
                if (!instance) {
                    instance = new App();
                }
                return instance;
            }
        };
    })();

    //생성
    const fnSingleTon = function () {

        const saveSingleTon = SingleTon.getInstance();
        const data = new Object();
        data.valid = true;
        data.state = 'first';
        saveSingleTon.save('/main/pythonTest.do',data)
            .then(function(text) {
                alert(text);
            });
    }


</script>