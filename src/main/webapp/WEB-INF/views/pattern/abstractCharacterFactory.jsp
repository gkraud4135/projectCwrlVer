<script>
    $(document).ready(function() {

        // 코드 확인 버튼
        $("#codeCheck").on("click", function () {
            let tf = $("#codeBox").is(':visible');
            if(tf){
                $("#codeBox").hide();
            } else {
                $("#codeBox").show();
            }
        });

        // 생성자 생성 (값)
        $("#addJob").on("click", function () {

            let jobName = $('#jobName');

            if(jobName.val() ==""){
                alert("값을 입력해주세요");
                return true;
            }

            let teamName = $('input[name=team]:checked').val();
            if(!teamName) {
                alert("팀을 선택 해주세요");
                return true;
            }
            console.log(teamName);
            if(teamName == "BLUE") {
                abstractCharacterFactory.addJob(jobName.val(), BlueTeam);
            } else {
                abstractCharacterFactory.addJob(jobName.val(), RedTeam);
            }

            let optObj = $('<option>', {
                value: jobName.val(),
                text: jobName.val() +"["+teamName+"]"
            });

            $('#character').append(optObj);
            $("#jobName").val('');

            alert("생성되었습니다");
        })

        //캐릭터 생성
        $("#createCharacter").on("click", function () {

            let characterName = $('#characterName');
            let selectedJob = $("#character option:selected");

            if(characterName.val() ==""){
                alert("값을 입력해주세요");
                return true;
            } else if (selectedJob.val()=='none') {
                alert("직업을 선택해주세요");
                return true;
            }

            let newchar = abstractCharacterFactory.create(selectedJob.val(), { name: characterName.val() });
            let inform = "fnInform('"+newchar.teamName+"','"+newchar.name+"')";
            let liObj = $('<li>');
            let aObj = $('<a>', {
                text: "["+newchar.teamName+"] "+"Name :"+characterName.val() + " Job :" + selectedJob.val(),
                href:"javascript:void(0);",
                onclick:inform
            });

            liObj.append(aObj);
            $('#characterBox').append(liObj);
            $("#characterName").val('');

            newchar.inform();
        });

    });

    let abstractCharacterFactory = (function() {
        let jobs = {};
        return {
            addJob: function(job, Character) {
                if (Character.prototype.inform) { // inform 메소드가 있어야만 등록 가능
                    jobs[job] = Character;
                }
            },
            create: function(job, options) {
                let Character = jobs[job];
                return (Character ? new Character(options) : null);
            }
        };
    })();

    let RedTeam = (function() {
        function RedTeam(options) {
            this.name = options.name;
            this.teamName = 'RED';
        }
        RedTeam.prototype.inform = function() {
            alert(this.teamName + '팀 소속' + this.name + '입니다');
        };
        return RedTeam;
    })();

    let BlueTeam = (function() {
        function BlueTeam(options) {
            this.name = options.name;
            this.teamName = 'BLUE';
        }
        BlueTeam.prototype.inform = function() {
            alert(this.teamName + '팀 소속' + this.name + '입니다');
        };
        return BlueTeam;
    })();

    let fnInform = function (teamName,name) {
        alert(teamName + '팀 소속' + name + '입니다');
    }



</script>